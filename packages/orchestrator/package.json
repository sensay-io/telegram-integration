{
  "name": "@sensay/orchestrator",
  "type": "module",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "author": "Sensay",
  "keywords": ["bots", "telegram", "sensay"],
  "description": "A service to run Telegram bots in cluster mode",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.13.11"
  },
  "scripts": {
    "build": "esbuild src/index.ts src/start_worker.ts --target=node22 --bundle --keep-names --platform=node --format=esm --packages=external --outdir=dist",
    "start": "node dist/index.js",
    "dev": "bun run --env-file=.env.local src/index.ts",
    "test": "bun --env-file=.env.local test"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.0",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-openapi": "^0.19.2",
    "@sensay/bot": "*",
    "hono": "^4.7.5",
    "hono-pino": "^0.8.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "pino-sentry": "^0.15.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "esbuild": "0.25.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "3.0.9"
  }
}
